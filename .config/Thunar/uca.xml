<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Открыть терминал здесь</name>
	<unique-id>1593694320754348-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>system-file-manager</icon>
	<name>Открыть thunar от рута</name>
	<unique-id>1593694320754389-2</unique-id>
	<command>pkexec thunar %f</command>
	<description>Open directory as root</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>catfish</icon>
	<name>Поиск файлов</name>
	<unique-id>1593694320754434-3</unique-id>
	<command>catfish --hidden %f</command>
	<description>Search files Catfish</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>applications-system</icon>
	<name>Проверка sha256 сумм</name>
	<unique-id>1593694320754471-4</unique-id>
	<command>yad --image=dialog-info --button=OK --title=&quot;Check sha256 for %n&quot; --text=&quot;$(sha256sum %f)&quot; --borders=20 --center</command>
	<description></description>
	<patterns>*.iso;*.img;*.sub</patterns>
	<other-files/>
</action>
</actions>
